# ===========================================
# Pipeline Notify MCP - Environment Variables
# ===========================================
# Copy this file to .env and fill in your values

# ─────────────────────────────────────────────
# Jenkins Configuration
# ─────────────────────────────────────────────
JENKINS_BASE_URL=https://jenkins.service.ntd.ciscolabs.com
JENKINS_USER=your_username
JENKINS_API_TOKEN=your_api_token
JENKINS_JOB_PATH_FXOS_PB=job/FXOS/job/FXOS_PRECOMMIT_BAZEL
JENKINS_JOB_PATH_ASA=job/ASA/job/Pre-commit-bazel

# ─────────────────────────────────────────────
# Splunk Configuration
# ─────────────────────────────────────────────
SPLUNK_HOST=http://bgl-vms-vm1221:8089
SPLUNK_TOKEN=your_splunk_token

# ─────────────────────────────────────────────
# Swarm Configuration (for delta change detection)
# ─────────────────────────────────────────────
SWARM_BASE_URL=https://sp4-fp-swarm.cisco.com
SWARM_API_TOKEN=your_swarm_token

# ─────────────────────────────────────────────
# Webex Bot Configuration
# ─────────────────────────────────────────────
WEBEX_BOT_TOKEN=your_webex_bot_token
WEBEX_WEBHOOK_SECRET=your_webhook_secret
WEBEX_BOT_EMAIL=your_bot@webex.bot

# ─────────────────────────────────────────────
# Server Configuration
# ─────────────────────────────────────────────
BOT_PORT=5000
MCP_SERVER_PORT=8080
DEBUG=false

# ─────────────────────────────────────────────
# Logging
# ─────────────────────────────────────────────
LOG_LEVEL=INFO
PYTHONUNBUFFERED=1
